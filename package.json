{
  "name": "npm-template",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.12.3",
  "description": "npm template",
  "author": "goodswifter <104248086@qq.com>",
  "license": "MIT",
  "homepage": "https://gitee.com/zad-front-end/npm-template",
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/zad-front-end/npm-template"
  },
  "keywords": [
    "template"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "dev:stub": "unbuild --stub",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@types/node": "^24.0.4",
    "@vue/compiler-sfc": "^3.5.17",
    "bumpp": "^10.2.0",
    "eslint": "^9.29.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
